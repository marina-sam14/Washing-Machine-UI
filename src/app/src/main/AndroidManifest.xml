<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.washingmachine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/images"
        android:supportsRtl="true"
        android:theme="@style/Theme.WashingMachine.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.WashingMachine.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstFragment" />
        <activity android:name=".Settings" />
        <activity android:name=".Laundring" />
        <activity android:name=".Pause" />
        <activity android:name=".PopUpCanceling" />
        <activity android:name=".Cancelled" />
        <activity android:name=".Helper" />

    </application>

</manifest>